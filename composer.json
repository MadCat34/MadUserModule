{
  "name": "madcat34/mad-user",
  "description": "A generic user registration and authentication module for ZF3 with Doctrine2.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "zf3"
  ],
  "homepage": "https://github.com/MadCat34/MadUserModule",
  "authors": [
    {
      "name": "Romain BASTIDE",
      "email": "romain.bastide@orange.fr",
      "homepage": "https://php-underground.blogspot.com"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",
    "zendframework/zend-authentication": "^2.5",
    "zendframework/zend-crypt": "^3.2",
    "zendframework/zend-form": "^2.10",
    "zendframework/zend-inputfilter": "^2.7",
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-modulemanager": "^2.8",
    "zendframework/zend-mvc": "^3.1",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-stdlib": "^3.1",
    "zendframework/zend-validator": "^2.10",
    "zendframework/zend-view": "^2.9",
    "zendframework/zend-session" : "^2.8",
    "zendframework/zend-http" : "^2.7",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
    "zendframework/zend-i18n": "^2.7",
    "zendframework/zend-mvc-plugin-prg": "^1.0",
    "zendframework/zend-hydrator": "^2.3"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^6.4",
    "phpmd/phpmd" : "^2.6",
    "squizlabs/php_codesniffer" : "^2.7",
    "zendframework/zend-captcha" : "^2.7",
    "zendframework/zend-coding-standard" : "^1.0"
  },
  "suggest": {
    "zendframework/zend-captcha" : "Zend\\Captcha if you want to use the captcha component"
  },
  "autoload": {
    "psr-0": {
      "MadUser": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}